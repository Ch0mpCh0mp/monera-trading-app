// Prisma schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Trade {
  id        String   @id @default(cuid())
  symbol    String
  side      String
  quantity  Decimal  @db.Decimal(18, 8)
  price     Decimal  @db.Decimal(18, 8)
  createdAt DateTime @default(now())
}